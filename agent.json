{
    "$schema": "http://www.lavishsoft.com/schema/agent.json",
    "name": "Sixty Sixty - Sixty - Twenty",
    "platforms": {
        "joe multiboxer session": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run SixtySixtySixtyTwenty.Session.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [
                        "endscript SixtySixtySixtyTwenty.Session"
                    ]
                }
            }
        },
        "joe multiboxer uplink": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run SixtySixtySixtyTwenty.Uplink.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [
                        "endscript SixtySixtySixtyTwenty.Uplink"
                    ]
                }
            }
        }

    }
}